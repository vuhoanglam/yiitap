{
	"name": "yiitap-icon",
	"prefix": "icon",
	"inputDir": "./src/svg",
	"outputDir": "./dist",
	"fontTypes": ["ttf", "woff", "woff2"],
	"assetTypes": ["ts", "css", "json", "html"],
	"normalize": true,
	"descent": 45,
	"formatOptions": {
		"json": {
			"indent": 2
		},
		"svg": {
			"centerHorizontally": true,
			"centerVertically": true
		}
	},
	"templates": {
		"css": "./src/template/css.hbs",
		"html": "./src/template/html.hbs"
	}
}
